{
  "name": "rainy-rtp",
  "version": "0.0.1",
  "description": "DTLS protocol implementation for Node.js written in TypeScript.",
  "homepage": "https://github.com/shinyoshiaki/rainy-dtls",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinyoshiaki/rainy-dtls.git"
  },
  "license": "MIT",
  "author": {
    "name": "shinyoshiaki"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc",
    "ci": "yarn type && yarn lint && yarn test",
    "fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "publish": "npm publish --access=public",
    "test": "jest --forceExit",
    "type": "tsc --noEmit -p .",
    "unused": "organize-imports-cli src/**/*.ts"
  },
  "dependencies": {
    "binary-data": "^0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.2"
  },
  "engines": {
    "node": ">=10"
  },
  "readme": "README.md"
}
